#include<stdio.h>
main()
{
    int n,i,j,c;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    if(n<3)
    {
        printf("%d",a[n-1]);
    }
    else
    {
    c=0;
    while(i<n)
    {
        a[i]=0;
    for(j=c;j<i;j++)
    {
        
        a[i]=a[i]+a[j];
    }
    i++;
    c++;
    }
    printf("%d",a[i-1]);
    }
}
